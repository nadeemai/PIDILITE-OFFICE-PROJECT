<mvc:View
    controllerName="com.tableentry.tablestructure.controller.Table_Entry"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:f="sap.f"
    xmlns:u="sap.ui.unified"
    xmlns:form="sap.ui.layout.form"
    xmlns:sf="sap.ui.comp.smartfield"
    xmlns:layout="sap.ui.layout"
    displayBlock="true"
    class="dashboardView">

    <Panel class="dashboardPanel" expandable="false">
        <headerToolbar>
            <Toolbar class="headerToolbar">
                <Image src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQN4zI47qvXfN00zC2FBC8aRP0kB6wtIcWTRQ&amp;s" 
                       width="150px" height="70px" class="logo"/>
                <Title text="Pidilite Price Requisition" class="headerTitle" />
            </Toolbar>
        </headerToolbar>
        <VBox class="sapUiSmallMargin">

            <!-- Header Tiles -->
            <HBox justifyContent="Center" alignItems="Center" class="tileContainer">
                <GenericTile header="Pending" class="customTile pendingTile">
                    <TileContent>
                        <NumericContent value="{/pendingCount}" valueColor="Critical" withMargin="false" class="centeredNumericContent"/>
                    </TileContent>
                </GenericTile>
                <GenericTile header="Approved" class="customTile approvedTile">
                    <TileContent>
                        <NumericContent value="{/approvedCount}" valueColor="Good" withMargin="false" class="centeredNumericContent"/>
                    </TileContent>
                </GenericTile>
                <GenericTile header="Rejected" class="customTile rejectedTile">
                    <TileContent>
                        <NumericContent value="{/rejectedCount}" valueColor="Error" withMargin="false" class="centeredNumericContent"/>
                    </TileContent>
                </GenericTile>
            </HBox>

            <!-- Action Buttons -->
            <HBox class="sapUiSmallMarginBottom actionButtons" alignItems="Center" justifyContent="End">
                <Button icon="sap-icon://hint" press="onInfoPress" />
                <Button icon="sap-icon://filter" text="Filter" press="onFilterPress" />
                <Button id="createButton" text="Create" type="Emphasized" press="onCreatePress" icon="sap-icon://add" class="createButton" />
            </HBox>

            <!-- Table -->
            <Table id="dashboardTable" items="{/requests}" mode="SingleSelectMaster" itemPress="onItemPress">
                <columns>
                    <Column><Text text="Title" /></Column>
                    <Column><Text text="Created By" /></Column>
                    <Column><Text text="Created On" /></Column>
                    <Column><Text text="Status" /></Column>
                    <Column><Text text="Modified On" /></Column>
                    <Column><Text text="Creator Comments" /></Column>
                    <Column><Text text="Reference No." /></Column>
                    <Column><Text text="Level 1 Approver" /></Column>
                    <Column><Text text="Level 2 Approver" /></Column>
                    <Column><Text text="Level 3 Approver" /></Column>
                </columns>
                <items>
                    <ColumnListItem>
                        <cells>
                            <Button text="View" press="onItemView" />
                            <Text text="{createdBy}" />
                            <Text text="{createdOn}" />
                            <ObjectStatus text="{status}" 
                                state="{= ${status} === 'Approved' ? 'Success' : 
                                       ${status} === 'Rejected' ? 'Error' : 'Warning'}" />
                            <Text text="{modifiedOn}" />
                            <Text text="{creatorComments}" />
                            <Text text="{referenceNo}" />
                            <Text text="{level1}" />
                            <Text text="{level2}" />
                            <Text text="{level3}" />
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>

            <!-- Price Requisition Dialog -->
            <Dialog id="priceRequisitionDialog" title="Price Requisition" contentWidth="800px" stretch="true">
                <content>
                    <VBox class="sapUiSmallMargin">
                        <Toolbar>
                            <ToolbarSpacer/>
                            <Button icon="sap-icon://save" text="Save" press="onSavePriceReq" type="Emphasized"/>
                            <Button icon="sap-icon://print" text="Print" press="onPrintPriceReq"/>
                            <Button icon="sap-icon://decline" text="Exit" press="onClosePriceReq"/>
                            <Button icon="sap-icon://edit" text="Edit" press="onEditPriceReq"/>
                        </Toolbar>
                        
                        <Panel headerText="Price Requisition Details" expandable="true" expanded="true">
                            <form:SimpleForm editable="true" layout="ResponsiveGridLayout">
                                <Label text="Created By:"/>
                                <Text text="{/selectedRequest/createdBy}"/>
                                
                                <Label text="Reference No.:"/>
                                <Text text="{/selectedRequest/referenceNo}"/>
                                
                                <Label text="Division:" required="true"/>
                                <Select selectedKey="{/selectedRequest/division}">
                                    <core:Item key="Division1" text="Division1"/>
                                    <core:Item key="Division2" text="Division2"/>
                                </Select>
                                
                                <Label text="DCNR% form:" required="true"/>
                                <Select selectedKey="{/selectedRequest/dcnrForm}">
                                    <core:Item key="20" text="20"/>
                                    <core:Item key="30" text="30"/>
                                </Select>
                                
                                <Label text="Standard Reason:" required="true"/>
                                <Select selectedKey="{/selectedRequest/standardReason}">
                                    <core:Item key="Nil" text="Nil"/>
                                    <core:Item key="Other" text="Other"/>
                                </Select>
                                
                                <Label text="Comment by Creator:" required="true"/>
                                <TextArea value="{/selectedRequest/creatorComments}" rows="3"/>
                                
                                <Label text="Created Date:"/>
                                <Text text="{/selectedRequest/createdOn}"/>
                                
                                <Label text="Status of the Request:"/>
                                <Text text="{/selectedRequest/status}"/>
                                
                                <Label text="Sales Group:" required="true"/>
                                <Select selectedKey="{/selectedRequest/salesGroup}">
                                    <core:Item key="Group1" text="Group1"/>
                                    <core:Item key="Group2" text="Group2"/>
                                </Select>
                                
                                <Label text="DCNR% to:" required="true"/>
                                <Select selectedKey="{/selectedRequest/dcnrTo}">
                                    <core:Item key="10" text="10"/>
                                    <core:Item key="15" text="15"/>
                                </Select>
                            </form:SimpleForm>
                        </Panel>
                        
                        <Panel headerText="Division Section Table" expandable="true" expanded="true">
                            <Table items="{/approvers}">
                                <columns>
                                    <Column><Text text="Role"/></Column>
                                    <Column><Text text="Approver"/></Column>
                                    <Column><Text text="Role"/></Column>
                                    <Column><Text text="Approver"/></Column>
                                </columns>
                                <items>
                                    <ColumnListItem>
                                        <cells>
                                            <Text text="Approver 1"/>
                                            <Text text="ayush.jha@sumo"/>
                                            <Text text="Alternate Approver 1"/>
                                            <Text text="tanl.singh@sumo"/>
                                        </cells>
                                    </ColumnListItem>
                                    <ColumnListItem>
                                        <cells>
                                            <Text text="Approver 2"/>
                                            <Text text="ayushi.khanolkar@sumo"/>
                                            <Text text="Alternate Approver 2"/>
                                            <Text text="aakib.mohd@sumo"/>
                                        </cells>
                                    </ColumnListItem>
                                    <ColumnListItem>
                                        <cells>
                                            <Text text="Approver 3"/>
                                            <Text text="yugal.kishore@sumo"/>
                                            <Text text="Alternate Approver 3"/>
                                            <Text text="sumit.jhaldiyal@sumo"/>
                                        </cells>
                                    </ColumnListItem>
                                </items>
                            </Table>
                        </Panel>
                    </VBox>
                </content>
            </Dialog>

            <!-- Application Status Dialog -->
            <Dialog id="appStatusDialog" title="Application Status" contentWidth="600px">
                <content>
                    <VBox class="sapUiSmallMargin">
                        <Text text="Created by: {/selectedRequest/createdBy}" class="sapUiSmallMarginBottom"/>
                        <Text text="On: {/selectedRequest/createdOn}" class="sapUiSmallMarginBottom"/>
                        
                        <Panel headerText="Approver 1" expandable="true" expanded="true">
                            <form:SimpleForm layout="ResponsiveGridLayout">
                                <Label text="Approver 1:"/>
                                <Text text="ayush.jha@sumo"/>
                                <Label text="Comments:"/>
                                <Text text="{/selectedRequest/approver1Comments}"/>
                            </form:SimpleForm>
                        </Panel>
                        
                        <Panel headerText="Alternate Approver 1" expandable="true" expanded="true">
                            <form:SimpleForm layout="ResponsiveGridLayout">
                                <Label text="Alternate Approver 1:"/>
                                <Text text="tanl.singh@sumo"/>
                                <Label text="Comments:"/>
                                <Text text="{/selectedRequest/altApprover1Comments}"/>
                            </form:SimpleForm>
                        </Panel>
                        
                        <Panel headerText="Approver 2" expandable="true" expanded="true">
                            <form:SimpleForm layout="ResponsiveGridLayout">
                                <Label text="Approver 2:"/>
                                <Text text="ayushi.khanolkar@sumo"/>
                                <Label text="Comments:"/>
                                <Text text="{/selectedRequest/approver2Comments}"/>
                            </form:SimpleForm>
                        </Panel>
                        
                        <Panel headerText="Alternate Approver 2" expandable="true" expanded="true">
                            <form:SimpleForm layout="ResponsiveGridLayout">
                                <Label text="Alternate Approver 2:"/>
                                <Text text="aakib.mohd@sumo"/>
                                <Label text="Comments:"/>
                                <Text text="{/selectedRequest/altApprover2Comments}"/>
                            </form:SimpleForm>
                        </Panel>
                        
                        <Panel headerText="Approver 3" expandable="true" expanded="true">
                            <form:SimpleForm layout="ResponsiveGridLayout">
                                <Label text="Approver 3:"/>
                                <Text text="yugal.kishore@sumo"/>
                                <Label text="Comments:"/>
                                <Text text="{/selectedRequest/approver3Comments}"/>
                            </form:SimpleForm>
                        </Panel>
                        
                        <Panel headerText="Alternate Approver 3" expandable="true" expanded="true">
                            <form:SimpleForm layout="ResponsiveGridLayout">
                                <Label text="Alternate Approver 3:"/>
                                <Text text="sumit.jhaldiyal@sumo"/>
                                <Label text="Comments:"/>
                                <Text text="{/selectedRequest/altApprover3Comments}"/>
                            </form:SimpleForm>
                        </Panel>
                    </VBox>
                </content>
                <endButton>
                    <Button text="Close" press="onCloseAppStatus" />
                </endButton>
            </Dialog>

            <!-- Filter Dialog -->
            <Dialog id="filterDialog" title="Filter Options" stretch="false" contentWidth="300px">
                <VBox class="sapUiSmallMargin">
                    <Label text="Type" />
                    <Select selectedKey="{/filter/type}" change="onFilterChange">
                        <items>
                            <core:Item key="" text="All" />
                            <core:Item key="Pending" text="Pending" />
                            <core:Item key="Approved" text="Approved" />
                            <core:Item key="Rejected" text="Rejected" />
                        </items>
                    </Select>
                    <Label text="Created By" />
                    <Select selectedKey="{/filter/createdBy}" change="onFilterChange">
                        <items>
                            <core:Item key="" text="All" />
                            <core:Item key="Ankit" text="Ankit" />
                            <core:Item key="Tuhin" text="Tuhin" />
                            <core:Item key="Nadeem" text="Nadeem" />
                            <core:Item key="Ishan" text="Ishan" />
                        </items>
                    </Select>
                    <Label text="Created On" />
                    <Select selectedKey="{/filter/createdOn}" change="onFilterChange">
                        <items>
                            <core:Item key="" text="All" />
                            <core:Item key="7-APR-2025" text="7-APR-2025" />
                        </items>
                    </Select>
                    <Label text="Status" />
                    <Select selectedKey="{/filter/status}" change="onFilterChange">
                        <items>
                            <core:Item key="" text="All" />
                            <core:Item key="Pending" text="Pending" />
                            <core:Item key="Approved" text="Approved" />
                            <core:Item key="Rejected" text="Rejected" />
                        </items>
                    </Select>
                    <Label text="Reference No." />
                    <Select selectedKey="{/filter/referenceNo}" change="onFilterChange">
                        <items>
                            <core:Item key="" text="All" />
                            <core:Item key="10211594" text="10211594" />
                            <core:Item key="10211592" text="10211592" />
                            <core:Item key="10211590" text="10211590" />
                        </items>
                    </Select>
                    <HBox justifyContent="End" class="sapUiSmallMarginTop">
                        <Button text="Apply" press="onApplyFilter" type="Emphasized" />
                        <Button text="Cancel" press="onCancelFilter" />
                    </HBox>
                </VBox>
            </Dialog>

            <!-- Upload Dialog -->
            <Dialog id="uploadDialog" title="Upload File" stretch="false" contentWidth="400px">
                <VBox class="sapUiSmallMargin">
                    <Text text="Attachments ({/attachmentCount})" class="sapUiSmallMarginBottom" />
                    <HBox>
                        <u:FileUploader
                            id="fileUploader"
                            name="myFileUpload"
                            uploadUrl="/upload"
                            tooltip="Upload your file"
                            uploadComplete="handleUploadComplete"
                            change="handleValueChange"
                            typeMissmatch="handleTypeMissmatch"
                            style="Emphasized"
                            fileType="pdf,jpg,png,doc,docx,xls,xlsx"
                            placeholder="Choose a file">
                            <u:parameters>
                                <u:FileUploaderParameter name="Accept-CH" value="Viewport-Width"/>
                                <u:FileUploaderParameter name="Accept-CH" value="Width"/>
                                <u:FileUploaderParameter name="Accept-CH-Lifetime" value="86400"/>
                            </u:parameters>
                        </u:FileUploader>
                        <Button text="Upload" press="handleUploadPress" type="Emphasized" />
                    </HBox>
                    <List id="attachmentList" items="{/uploads}" mode="Delete" delete="onUploadDelete">
                        <items>
                            <StandardListItem 
                                title="{fileName}"
                                description="{fileType}"
                                info="{fileSize}"
                                type="Active"
                                press="onAttachmentPress"
                                icon="{= ${fileType} === 'pdf' ? 'sap-icon://pdf-attachment' : 
                                       ${fileType} === 'jpg' || ${fileType} === 'png' ? 'sap-icon://picture' : 
                                       'sap-icon://document-text'}" />
                        </items>
                    </List>
                    <HBox justifyContent="End" class="sapUiSmallMarginTop">
                        <Button text="Close" press="onCloseUpload" />
                    </HBox>
                </VBox>
            </Dialog>
        </VBox>
    </Panel>
</mvc:View>


